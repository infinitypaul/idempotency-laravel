{
    "name": "infinitypaul/idempotency-laravel",
    "description": "Elegant and production-ready idempotency middleware for Laravel APIs.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Infinitypaul\\Idempotency\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Infinitypaul\\Idempotency\\IdempotencyServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "infinitypaul",
            "email": "infinitypaul@live.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
